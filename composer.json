{
    "name": "flatline/cfddns",
    "description": "Updater to use CloudFlare as a dynamic DNS service",
    "keywords":["cloudflare", "ddns", "dns"],
    "require": {
        "php": ">=5.4",
        "symfony/console": "2.4.*",
        "symfony/config": "2.4.*",
        "symfony/yaml": "2.4.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Longo",
            "email": "luciano.longo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Flatline\\CfDdns\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "bin": ["bin/cfddns"],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "mockery/mockery": "dev-master"
    }
}
